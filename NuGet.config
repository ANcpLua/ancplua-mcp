<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="dotnet10" value="https://pkgs.dev.azure.com/dnceng/public/_packaging/dotnet10/nuget/v3/index.json" />
  </packageSources>
  <packageSourceMapping>
    <!-- .NET 10 GA packages from dotnet10 feed (higher priority) -->
    <packageSource key="dotnet10">
      <package pattern="Microsoft.Extensions.*" />
      <package pattern="Microsoft.AspNetCore.*" />
      <package pattern="Microsoft.NET.*" />
      <package pattern="System.*" />
    </packageSource>
    <!-- nuget.org for all packages including Microsoft.Extensions.* (as fallback for
         transitive dependencies like *.Abstractions that may be older versions) -->
    <packageSource key="nuget.org">
      <package pattern="*" />
      <package pattern="Microsoft.Extensions.*" />
      <package pattern="Microsoft.AspNetCore.*" />
      <package pattern="Microsoft.NET.*" />
      <package pattern="System.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
